#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int sum1,diff1;
    float sum2,diff2;
	int a,b;
    float c,d;
    scanf("%d%d",&a,&b);
    sum1=a+b;
    diff1=a-b;
    scanf("%f%f",&c,&d);
    sum2=c+d;    
    diff2=c-d;
    printf("%d %d\n",sum1,diff1);
    printf("%.1f %.1f\n",sum2,diff2);
    return 0;
}
